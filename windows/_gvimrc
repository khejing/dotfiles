set go-=T
set go-=m
set go-=r
set go-=L
set nobackup
set undodir=~/.vimundodir
set encoding=utf-8
source $VIMRUNTIME/delmenu.vim
source $VIMRUNTIME/menu.vim
colorscheme murphy
au BufRead,BufNewFile *.txt set filetype=tex inde=
au TerminalOpen * call term_sendkeys('', "cd Documents\<CR>")
set ignorecase smartcase
unmap <C-F>
unmap <C-Y>
map ： :
map ？ ?
map 、 /
map F. F。
map f. f。
map F, F，
map f, f，
map j gj
map k gk
tmap <C-V> <C-W>"*
tmap <C-Y> <C-W>""
command UbuntuTerm :term ubuntu1804.exe
